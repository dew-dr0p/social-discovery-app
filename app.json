{
  "expo": {
    "name": "Social Discovery App",
    "slug": "social-discovery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "com.dewdr0p.socialdiscoveryapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "social-discovery-app",
    "plugins": ["expo-router"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "015e8f8e-eb9e-4f21-aca9-edb7e915bcf1"
      }
    }
  }
}
